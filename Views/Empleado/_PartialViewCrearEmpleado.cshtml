@model ProyectoTallerMecanico.Models.Empleado

<div class="row">
	<form method="post" asp-controller="Empleado" asp-action="Create" class="col s12">
		<div class="input-field col s6">
			<i class="material-icons">account-circle</i>
			<label asp-for="Nombre"></label>
			<input type="text" asp-for="Nombre" placeholder="Ingrese nombre de Empleado" class="validate"/>
		</div>
		<div class="input-field col s6">
			<i class="material-icons">directions</i>
			<label asp-for="Direccion"></label>
			<input type="text" asp-for="Direccion" class="validate"/>
		</div>
		<div class="input-field col s6">
			<i class="material-icons">contact_phone</i>
			<label asp-for="Telefono"></label>
			<input asp-for="Telefono"/>
		</div>
		<div class="input-field col s6">
			<i class="material-icons">attach_money</i>
			<label asp-for="Sueldo"></label>
			<input asp-for="Sueldo" />
		</div>
	</form>
</div>